ALGORITHM algo2
VAR
    set1 : ARRAY_OF INTEGER[n];
    set2 : ARRAY_OF INTEGER[m];
    i,j:INTEGER
    sum:INTEGER

BEGIN
    sum=0
    FOR i FROM 0 TO set1.length STEP 1  DO
        Read(set1[i])
    END_FOR
    FOR j FROM 0 TO set2.length STEP 1  DO
        Read(set2[j])
    END_FOR
    
    FOR i FROM 0 TO set1.length STEP 1  DO
        FOR j FROM 0 TO set2.length STEP 1  DO
        
            IF (set1[i]<>set2[j]) THEN
                sum = sum+set1[i] BREAK;
            END_IF
        END_FOR
    END_FOR
END




